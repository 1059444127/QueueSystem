<?xml version="1.0"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
  <connectionStrings>
    <!--服务端数据库地址。其中name不可修改-->
    <add name="Server" connectionString="Database='dbserver';Data Source='cysoft.uicp.net';User Id='root';Password='admin88';charset='utf8';pooling=true" />
    <!--以下配置为本地数据库地址。其中name为地区编码-->
    <add name="15" connectionString="Database='QueueDB';Data Source='cysoft.uicp.net';User Id='root';Password='admin88';charset='utf8';pooling=true" />
    <add name="21" connectionString="Database='queuedb0208';Data Source='cysoft.uicp.net';User Id='root';Password='admin88';charset='utf8';pooling=true" />
  </connectionStrings>
  <appSettings>
    <!--基础数据同步间隔，单位 秒 如果对实时性要求不高，请间隔调大一些以减轻数据库压力-->
    <add key ="BasicInterval" value ="25"/>
    <!--业务数据同步间隔，单位 秒 如果对实时性要求不高，请间隔调大一些以减轻数据库压力-->
    <add key ="BusyInterval" value ="8"/>
  </appSettings>
</configuration>
